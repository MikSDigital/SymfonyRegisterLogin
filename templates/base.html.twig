<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8">
    <title>{% block title %}Welcome!{% endblock %}</title>
    {% block stylesheets %}{% endblock %}
</head>
<body>

{% if app.user %}
    <a href="{{ path('logout') }}">Logout</a>
{% else %}
    <a href="{{ path('security_register') }}">Register</a>
    <a href="{{ path('security_login') }}">Login</a>
{% endif %}

{% block body %}{% endblock %}
{% block javascripts %}{% endblock %}
</body>
</html>
